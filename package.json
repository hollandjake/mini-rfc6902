{
  "name": "mini-rfc6902",
  "version": "0.2.3",
  "description": "TypeScript implementation of RFC6902 with mini-rfc6902 format support",
  "license": "MIT",
  "author": "Jake Holland",
  "homepage": "https://github.com/hollandjake/mini-rfc6902#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hollandjake/mini-rfc6902.git"
  },
  "bugs": {
    "url": "https://github.com/hollandjake/mini-rfc6902/issues"
  },
  "type": "module",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "scripts": {
    "build": "tshy",
    "clean": "gts clean",
    "clean:build": "pnpm clean && pnpm build",
    "lint": "gts lint",
    "lint:fix": "gts fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "types": "./dist/commonjs/index.d.ts",
  "dependencies": {
    "just-clone": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@vitest/coverage-istanbul": "^2.1.6",
    "gts": "^5.3.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tshy": "^3.0.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6"
  },
  "keywords": [
    "diff",
    "json patch",
    "rfc6902"
  ],
  "engines": {
    "node": ">=10.4.0"
  },
  "readme": "./README.md",
  "tshy": {
    "exclude": [
      "src/**/*.test.ts"
    ],
    "selfLink": false,
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  }
}
