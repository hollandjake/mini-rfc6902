{
  "name": "mini-rfc6902",
  "version": "0.5.0",
  "description": "TypeScript implementation of RFC6902 with mini-rfc6902 format support",
  "license": "MIT",
  "author": "Jake Holland",
  "homepage": "https://github.com/hollandjake/mini-rfc6902#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hollandjake/mini-rfc6902.git"
  },
  "bugs": {
    "url": "https://github.com/hollandjake/mini-rfc6902/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "gts clean",
    "clean:build": "pnpm clean && pnpm build",
    "lint": "gts lint",
    "lint:fix": "gts fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "validate": "attw --pack"
  },
  "types": "./dist/index.d.mts",
  "peerDependencies": {
    "bson": "*"
  },
  "peerDependenciesMeta": {
    "bson": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.13.5",
    "@vitest/coverage-istanbul": "^3.0.7",
    "gts": "^6.0.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.1",
    "vite-plugin-require-transform": "^1.0.21",
    "vitest": "^3.0.7"
  },
  "keywords": [
    "diff",
    "json patch",
    "rfc6902"
  ],
  "engines": {
    "node": ">=11.0.0"
  },
  "readme": "./README.md"
}
